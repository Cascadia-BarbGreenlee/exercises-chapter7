<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout tools:context=".SensorActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
xmlns:tools="http://schemas.android.com/tools" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    
        <RelativeLayout android:layout_height="wrap_content" 
        android:layout_width="fill_parent" 
        android:background="@color/navbar_background" 
        android:layout_alignParentBottom="true" 
        android:id="@+id/navbar">
        
        <ImageButton android:layout_height="wrap_content" 
            android:layout_width="wrap_content" 
            android:id="@+id/imageButtonList" 
            android:src="@drawable/contactlisticon"
            android:layout_toLeftOf="@+id/imageButtonMap" 
            android:layout_marginRight="20dp" 
            android:layout_centerVertical="true"/>
        
        <ImageButton android:layout_height="wrap_content" 
            android:layout_width="wrap_content" 
            android:id="@+id/imageButtonMap" 
            android:src="@drawable/mapicon" 
            android:layout_centerVertical="true" 
            android:layout_centerHorizontal="true"/>
        
        <ImageButton android:layout_height="wrap_content" 
            android:layout_width="wrap_content" 
            android:id="@+id/imageButtonSettings" 
            android:src="@drawable/settingsicon" 
            android:layout_centerVertical="true" 
            android:layout_toRightOf="@+id/imageButtonMap" 
            android:layout_marginLeft="20dp"/>
    </RelativeLayout>
    
<TextView
        android:id="@+id/textNetwork"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:text="Network Sensor"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textNetwork"
        android:layout_below="@+id/textNetwork"
        android:text="Latitude:" />
    
    <TextView
        android:id="@+id/textLatitudeNet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:text="" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:text="Longitude:" />

    <TextView
        android:id="@+id/textLongitudeNet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_toRightOf="@+id/textView4"
        android:text="" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textNetwork"
        android:layout_below="@+id/textView4"
        android:text="Accuracy:" />
    
    <TextView
        android:id="@+id/textAccuracyNet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:text="" />
    
    <TextView
        android:id="@+id/textGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="21dp"
        android:text="GPS Sensor"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textGPS"
        android:layout_below="@+id/textGPS"
        android:text="Latitude:" />
    
    <TextView
        android:id="@+id/textLatitudeGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:text="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textGPS"
        android:layout_below="@+id/textView1"
        android:text="Longitude:" />
    
    <TextView
        android:id="@+id/textLongitudeGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView3"
        android:layout_alignTop="@+id/textView3"
        android:text="" />  

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textGPS"
        android:layout_below="@+id/textView3"
        android:text="Accuracy:" />
    
    <TextView
        android:id="@+id/textAccuracyGPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:text="" />

    
    <TextView
        android:id="@+id/textBest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="41dp"
        android:text="Best Location"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textBest"
        android:layout_below="@+id/textBest"
        android:text="Latitude:" />
    
    <TextView
        android:id="@+id/textLatitudeBest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:text="" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textBest"
        android:layout_below="@+id/textView7"
        android:text="Longitude:" />
    
    <TextView
        android:id="@+id/textLongitudeBest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:text="" />  

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textBest"
        android:layout_below="@+id/textView8"
        android:text="Accuracy:" />
    
    <TextView
        android:id="@+id/textAccuracyBest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:text="" />
</RelativeLayout>